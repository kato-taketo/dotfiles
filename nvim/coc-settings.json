{
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls-root", "compile_commands.json"],
      "initializationOptions": {
         "cache": {
           "directory": "/tmp/ccls-cache"
         }
       }
    },
    "golang": {
      "command": "/Users/t-kato/go/bin/gopls",
      "filetypes": ["go"],
      "rootPatterns": ["go.mod", "go.sum", ".git/"]
    }
  }
}
