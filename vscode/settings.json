{
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.minimap.enabled": false,
  "editor.lineHeight": 22,
  "editor.fontSize": 13,
  "editor.fontFamily": "HackGenNerd",
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "workbench.colorTheme": "Iceberg",
  "workbench.iconTheme": "vscode-icons",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/usr/local/bin/zsh"
    }
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "files.insertFinalNewline": true,
  "vim.useSystemClipboard": false,
  "vim.highlightedyank.enable": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["っ", "ｊ"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["<ctrl+h>"],
      "after": ["^"]
    },
    {
      "before": ["<ctrl+l>"],
      "after": ["$"]
    },
    {
      "before": ["s", "h"],
      "after": ["<ctrl+w>", "h"]
    },
    {
      "before": ["s", "l"],
      "after": ["<ctrl+w>", "l"]
    },
    {
      "before": ["s", "v"],
      "after": ["<ctrl+w>", "v"]
    },
    {
      "before": ["s", "p"],
      "after": ["<ctrl+w>", "p"]
    },
    {
      "before": ["<C-w>", "v"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<C-w>", "p"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<C-k>"],
      "after": ["g", "b"]
    },
    {
      "before": ["t", "h"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["t", "l"],
      "commands": ["workbench.action.nextEditor"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<ctrl+h>"],
      "after": ["^"]
    },
    {
      "before": ["<ctrl+l>"],
      "after": ["$"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["<C-k>"],
      "after": ["g", "b"]
    }
  ],
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.useCtrlKeys": true,
  "vim.statusBarColorControl": false,
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    }
  ],
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.quickSuggestionsDelay": 1000,
  "indentRainbow.updateDelay": 800,
  "vsicons.dontShowNewVersionMessage": true,
  "window.zoomLevel": 1,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.chat.localeOverride": "ja",
  "github.copilot.editor.enableAutoCompletions": true,
  "application.shellEnvironmentResolutionTimeout": 30,
  "go.testEnvFile": "${workspaceFolder}/.env.private",
  "go.gopath": "/Users/t-kato/go",
  "go.formatTool": "goimports",
  "go.lintTool": "golangci-lint",
  "go.buildTags": "",
  "go.toolsManagement.checkForUpdates": "local",
  "go.useLanguageServer": true,
  "go.alternateTools": {
    "go": "/opt/homebrew/bin/go"
  },
  "go.goroot": "",
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "[go.mod]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.showPushSuccessNotification": true,
  "scm.diffDecorations": "all",
  "scm.diffDecorationsGutterVisibility": "always"
}
